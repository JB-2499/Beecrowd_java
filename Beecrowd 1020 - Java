import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner pergunta = new Scanner(System.in);
        int n = pergunta.nextInt();
        System.out.println(n/365+" ano(s)\n"+(n%365)/30+" mes(es)\n"+(n%365)%30+" dia(s)");
        pergunta.close();
    }
}
